<launch>
    <param name="use_sim_time" value="true" />
    <arg name="world" default="" doc="Path to the world to load"/>
    <arg name="mode" default="realtime" doc="Startup mode"/>
    <arg name="no_gui" default="false" doc="Start Webots with minimal GUI"/>
    <arg name="stream" default="false" doc="Start Webots streaming server"/>
    <node name="webots_launcher" pkg="corgi_sim" type="webots_launcher.py" args="--world=$(arg world) --mode=$(arg mode) --no-gui=$(arg no_gui) --stream=$(arg stream)" required="true"/>
    <node name="corgi_sim" pkg="corgi_sim" type="corgi_sim" respawn="true"/>
</launch>